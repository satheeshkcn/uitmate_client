{

	"name": "XPath_SK",
	"version": "1.1",
	"description": "Get XPath of element on click. It shows multiple versions of XPath & CSS Path to pick and choose the one that fit individual needs  ",
	"icons": { 
		"16": "img/icon16.png",		
		"19": "img/icon19.png",		
		"32": "img/icon32.png",		
		"48": "img/icon48.png",		
		"128": "img/icon128.png"	
	},
	"background": {"page":"background.htm"},
	
	"omnibox": { "keyword" : "t" },
	"permissions": [ 
		"*://*/*",
		"contextMenus",
		"unlimitedStorage",
		"notifications",
		"tabs"
	],
	"browser_action": {
		"default_icon": "img/icon19.png",
		"default_popup": "popup.htm",
		"default_title": "XPath | Satheesh"
	},
	"manifest_version": 2,
	"homepage_url": "http://google.com/"
}